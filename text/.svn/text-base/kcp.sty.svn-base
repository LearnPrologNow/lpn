%
\newcommand{\cHapter}[1]{%
\clearemptydoublepage
\thispagestyle{plain}
    \refstepcounter{chapter}%
    \addcontentsline{toc}{chapter}{% This is for the TOC
%       \MakeTextUppercase{\chaptername}~\thechapter\quad\MakeTextUppercase{#1}}
       \chaptername~\thechapter\quad\textbf{#1}}
    \vskip\baselineskip

%    \begin{center}                 % This is the Chapter heading
%    \large\MakeTextUppercase{\chaptername}~\thechapter
%    \vskip2\baselineskip
%       \Large\MakeTextUppercase{#1}
%    \end{center}

%    \marginpar{\rule{1mm}{55mm}}
    \vspace*{20mm}
    \begin{flushright}                 % This is the Chapter heading
    \Huge\chaptername~\thechapter
    \vskip2\baselineskip
    \Huge\textbf{#1}
    \end{flushright}
    \rule{110mm}{1mm}
%    \vskip3\baselineskip
%    \def\chaptermark{\@CMF~\MakeTextUppercase{#1}}
    \def\chaptermark{#1}
\@afterindentfalse
}


